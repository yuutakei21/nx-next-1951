{
  "name": "012-prisma-app",
  "version": "1.0.0",
  "description": "Prisma Health Check sample app",
  "license": "MIT",
  "scripts": {
    "prisma": "prisma generate",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "seed:dev": "ts-node prisma/prisma.seed",
    "openapi": "openapi -i http://localhost:3000/api-json -o ./openapi"
  },
  "dependencies": {
    "@nestjs/common": "10.3.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "10.3.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "10.3.1",
    "@nestjs/terminus": "10.2.1",
    "@prisma/client": "^5.17.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "jwt-decode": "^4.0.0",
    "nestjs-prisma": "^0.23.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^5.17.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "7.8.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/swagger": "^7.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "4.17.21",
    "@types/node": "20.10.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "openapi-typescript": "^7.1.0",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "^3.3.3",
    "prisma-class-generator": "^0.2.9",
    "source-map-support": "0.5.21",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3",
    "webpack": "^5.0.0"
  }
}
